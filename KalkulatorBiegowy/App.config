<?xml version="1.0"?>
<configuration>
  <system.web>
    <compilation debug="true"/>
  </system.web>
  <system.serviceModel>
    <services>
      <service name="KalkulatorBiegowyService.KalkulatorBiegowyService">
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8733/Design_Time_Addresses/KalkulatorBiegowyService/KalkulatorBiegowyService/"/>
          </baseAddresses>
        </host>
        <endpoint address="" binding="wsHttpBinding" contract="KalkulatorBiegowyService.IKalkulatorBiegowy">
